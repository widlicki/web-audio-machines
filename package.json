{
  "name": "web-audio-machines",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "db:seed": "yarn local api/database/seeds/seedDatabase.ts",
    "db:setup": "yarn db:create:dev && yarn db:create:test",
    "db:create:dev": "/library/PostgreSQL/13/bin/createdb --owner=postgres web-audio-machines -U postgres",
    "db:create:test": "createdb --owner=postgres web_audio_machines_test",
    "db:migrate:local": "yarn typeorm:local migration:run",
    "db:migrate:local:testing": "yarn typeorm:local:testing migration:run",
    "g:migration": "yarn typeorm:local migration:generate -n",
    "g:entity": "yarn typeorm:local entity:create -n",
    "typeorm:local": "yarn local ./node_modules/typeorm/cli.js",
    "local": "DOTENV_CONFIG_PATH=./.env ts-node -P ./tsconfig.yarn.json -r dotenv/config"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.7",
    "axios": "^0.24.0",
    "dotenv": "^14.3.2",
    "next": "12.0.7",
    "pg": "^8.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/jest": "^27.4.0",
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "chance": "^1.1.8",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.5",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "4.5.4"
  }
}
